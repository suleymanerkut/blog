@model IEnumerable<proje15haziran.Identity.ApplicationUser>
@{
    ViewBag.Title = "Users";
}
<div class="panel panel-primary">
    <div class="panel-heading"></div>
    Users
</div>
<table class="table table-striped">
    <tr>
        <th>Id</th>
        <th>Username</th>
        <th>Email</th>
    </tr>
    @if (Model.Count() == 0)
    {
        <tr>
            <td colspan="3" class="text-center">
                Kullanıcı Yok
            </td>
        </tr>
    }
    else
    {
        foreach (var item in Model)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.UserName</td>
                <td>@item.Email</td>          
            </tr>
        }
    }
</table>

